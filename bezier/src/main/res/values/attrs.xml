<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- *记住* declare-styleable的name就是自定义控件的名字 -->
    <declare-styleable name="EditTextCanClean">
        <attr name="cleanIconDrawable" format="reference"/>
        <attr name="hint" format="reference|string"/>
        <attr name="text" format="reference|string"/>
        <attr name="textSize" format="reference|dimension"/>
        <attr name="textColor" format="reference|color"/>
        <attr name="maxLength" format="integer"/>
    </declare-styleable>

</resources>
